{
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "clean-bundle": "rm -rf \"build/web_modules\"",
    "bundle:clean": "npm run clean-bundle && snowpack --bundle",
    "bundle": "rm -rf \"web-modules\" && node_modules/.bin/snowpack --bundle",
    "test": "echo \"This template does not include a test runner by default.\" && exit 1",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "prettier --check \"src/**/*.{ts,js}\"",
    "link": "npm link \"generative-algos\"",
    "webpack": "node_modules/.bin/webpack --config webpack.config.js --watch"
  },
  "dependencies": {
    "@snowpack/plugin-optimize": "^0.2.6",
    "canvas-confetti": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@snowpack/plugin-run-script": "^2.1.7",
    "@snowpack/plugin-typescript": "^1.0.2",
    "@snowpack/plugin-webpack": "^2.0.17",
    "@types/canvas-confetti": "^1.0.0",
    "@types/snowpack-env": "^2.3.0",
    "prettier": "^2.0.5",
    "snowpack": "^2.15.1",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack-cli": "^4.1.0"
  }
}
